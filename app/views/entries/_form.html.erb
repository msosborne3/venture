<%= form_for(@entry) do |f| %>

  <div class="field">
    <%= f.label :title %><br>
    <%= f.text_field :title %><br>
  </div>

  <div class="field">
    <%= f.label :content %><br>
    <%= f.text_area :content %><br>
  </div>

  <div class="actions">
    <%= f.submit "Create New Journal Entry" %>
  </div>

<% end %>